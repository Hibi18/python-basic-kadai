      "source": [
        "def calculate_total(product_price, tax):\n",
        "\n",
        "  tax_price = (tax / 100)*product_price\n",
        "  total = product_price + tax_price\n",
        "\n",
        "  return total\n",
        "\n",
        "total = calculate_total(1000, 10)\n",
        "\n",
        "print(f\"消費税込みで{total}円です\")"
      ]
