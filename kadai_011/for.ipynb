        "#forを使った処理\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "for i in array:\n",
        "  print(i)\n",
        "\n",
        "#whileを使った処理\n",
        "num = 0\n",
        "\n",
        "while num <= 8:\n",
        "  print(array[num])\n",
        "  num = num + 1"
